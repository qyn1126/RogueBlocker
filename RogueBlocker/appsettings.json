{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "EventLog": {
      "SourceName": "RogueBlocker",
      "LogName": "Application",
      "LogLevel": {
        "Default": "Warning"
      }
    }
  },
  "RogueBlocker": {
    "AllowedUsernames": [
      "admin"
    ],
    "FirewallRulePrefix": "RogueBlocker_Ban_",
    "LogBannedIps": true,
    "WhitelistedIps": [
      "127.0.0.1",
      "::1"
    ]
  }
}
